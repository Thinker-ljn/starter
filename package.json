{
  "name": "starter",
  "version": "0.0.0",
  "description": "a starter",
  "keywords": [
    "starter",
    "typescript"
  ],
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/starter.es.js",
      "require": "./dist/starter.cjs.js"
    }
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "eslint \"**/*.{ts,js}\" --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build:dev": "vite build --mode development",
    "build": "vite build && pnpm build:dev && pnpm build:dts",
    "build:dts": "tsup",
    "release": "node ./scripts/release.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "author": "Thinker-ljn",
  "license": "ISC",
  "devDependencies": {
    "@nexhome/yorkie": "^2.0.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/prettier": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^5.1.1",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "minimist": "^1.2.5",
    "prettier": "^2.4.1",
    "semver": "^7.3.5",
    "ts-jest": "^27.0.5",
    "tsup": "^5.2.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-dts": "^0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thinker-ljn/starter.git"
  },
  "homepage": "https://github.com/Thinker-ljn/starter#readme"
}
